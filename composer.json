{
    "name": "merax/tables",
    "description": "Merax Tables Generator Package",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "illuminate/contracts": "^6.6",
        "illuminate/support": "~5.6.34|~5.7.0|~5.8.0|^6.0",
        "spatie/laravel-query-builder": "~2.0"
    },
    "autoload": {
        "psr-4": {"MeraxTables\\": "src/"}
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\QueryBuilder\\QueryBuilderServiceProvider"
            ]
        }
    }
}
